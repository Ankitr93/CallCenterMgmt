<%
try {

Connection connection=DbConFactory.getCon();
String QueryString = "SELECT * from keyword where keyword='monitor'";
PreparedStatement pst = connection.prepareStatement(QueryString);
ResultSet rs=null;
rs = pst.executeQuery(QueryString);
%>
<font color="blue"><center><u><h2>Keyword Details </h2></u></center></font>

<table align="center" cellpadding="25" border="1" style="background-color: grey">

<tr>
